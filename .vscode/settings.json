{
    "files.exclude": {
        ".editorconfig": true,
        ".gitattributes": true,
        ".github": true,
        ".gitignore": true,
        "AppDeployToolkitLogo.png": true,
        "CHANGELOG.txt": true,
        "COPYING.Lesser": true,
        "docs": true,
        "Examples": true,
        "LICENSE": true,
        "material": true,
        "mkdocs.yml": true,
        "PSAppDeployToolkit.pdf": true,
        "README.md": true,
        "Sources": true,
        "Testing": true,
        "TODO.txt": true,
        "wiki": true,
        "**/Deploy-Application.exe": true,
        "**/Deploy-Application.exe.config": true,
        "**/thumbs.db": false
    },
    "terminal.integrated.profiles.windows": {
        "PowerShell": {
            "source": "PowerShell",
            "icon": "terminal-powershell"
        },
        "Powershell Bypass":{
            "path": "C:\\Windows\\System32\\WindowsPowerShell\\v1.0\\powershell.exe",
            "source": "PowerShell",
            "args": [
                "-NoLogo",
                "-NoExit",
                "-ExecutionPolicy",
                    "Unrestricted"
                    
            ]
        },
        "Command Prompt": null,
        "Git Bash": null
      
    },
    "terminal.integrated.defaultProfile.windows": "PowerShell",
    "files.defaultLanguage": "PowerShell",
    "terminal.integrated.enablePersistentSessions": false,
    "terminal.integrated.persistentSessionReviveProcess": "never",
    "powershell.scriptAnalysis.settingsPath": ".vscode\\PSScriptAnalyzerSettings.psd1"

    
}